{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "beyondcode/laravel-websockets": "^1.0",
        "fideloper/proxy": "^4.1",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^5.7",
        "laravel/tinker": "^1.0",
        "lorisleiva/laravel-deployer": "^0.2.6",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.3"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.2",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "laravel/homestead": "^8.1",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^2.1",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
